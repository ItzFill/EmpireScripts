local CoreGui = game:GetService("StarterGui")

CoreGui:SetCore("SendNotification", {
	Title = "Empire Scripts";
	Text = "Load auto-gifts // By ianphil#4488 ";
	Duration = 100;

})
getgenv().AutoGift = true

local Workspace = game:GetService("Workspace")
spawn(function()
    while AutoGift do
        wait(0.01)
local ohTable1 = {
	[1] = 1
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 2
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 3
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 4
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 5
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 6
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 7
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 8
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 9
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 10
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 11
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)

local ohTable1 = {
	[1] = 12
}

workspace.__THINGS.__REMOTES["redeem free gift"]:InvokeServer(ohTable1)
 
    end
end)
